HostName gitee.com
	User git
	IdentityFile C:\Users\00775626\.ssh\id_rsa.gitee
Host github.com
	HostName github.com
	User git
	IdentityFile C:\Users\00775626\.ssh\id_rsa.github
Host 108.160.139.217
